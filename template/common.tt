<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link rel="stylesheet" type="text/css" charset="utf-8" media="all" href="../css/typo.css" />
    <link rel="stylesheet" href="../css/simple.css" type="text/css" media="all" />
    <link rel="shortcut icon" href="../images/self.png" type="image/png" />
    <title>[% article.0.title %]</title>
  </head>
  <body class="container">
    <div class="typo typo-selection">
      <div class="title">
        <h1>[% article.0.title %]</h1>
      </div>
      <ol start="0">
        <li><a href="../index/index.html">首页</a></li>
        <li><a href="./index.html">[% article.0.title %]</a>
          [% section_no = 1 %]
          [% FOREACH section IN article.slice(1,-1) %]
          <ul>
            <li><a href="#section[% section_no %]">[% section.section.0.caption %]</a></li>
          </ul>
          [% section_no = section_no+1 %]
          [% END %]
        </li>
      </ol>
      <br />
      [% section_no = 1 %]
      [% FOREACH section IN article.slice(1,-1) %]
      <div class="section">
        <div class="heading">
          <h2 id="section[% section_no %]">[% section.section.0.heading %]</h2>
        </div>
        [% FOREACH part IN section.section.slice(1,-1) %]
        [% IF part.paragraph %]
        <div class="paragraph">
          <p>
            [% part.paragraph %]
          </p>
        </div>
        [% ELSIF part.list %]
        <div class="entrys">
          <ol>
            [% FOREACH item IN part.list %]
            <li>[% item.item.0.entry %]</li>
            [% FOREACH item_part IN item.item.slice(1,-1) %]
            [% IF item_part.paragraph %]
            <div class="paragraph">
              <p>
                [% item_part.paragraph %]
              </p>
            </div>
            [% ELSIF item_part.list %]
            <div class="entrys">
              <ol>
                [% FOREACH item_item IN item_part.list %]
                <li>[% item_item.item.0.entry %]</li>
                [% FOREACH item_item_part IN item_item.item.slice(1,-1) %]
                [% IF item_item_part.paragraph %]
                <div class="paragraph">
                  <p>
                    [% item_item_part.paragraph %]
                  </p>
                </div>
                [% END %]
                [% END %]
                [% END %]
              </ol>
            </div>
            [% END %]
            [% END %]
            [% END %]
          </ol>
        </div>
        [% END %]
        [% END %]
      </div>
      <br />
      [% section_no = section_no+1 %]
      [% END %]
    </div>

    <div id="disqus_thread"></div>
    <script type="text/javascript">
      /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
      var disqus_shortname = 'self-wiki'; // required: replace example with your forum shortname

      /* * * DON'T EDIT BELOW THIS LINE * * */
      (function() {
      var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
      dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
      (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
      })();
    </script>
    <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
    <a href="http://disqus.com" class="dsq-brlink">comments powered by <span class="logo-disqus">Disqus</span></a>

  </body>
</html>
